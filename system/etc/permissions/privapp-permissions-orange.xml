<!--
This XML file declares which signature|privileged permissions should be granted to privileged
applications that come with the platform
-->
<permissions>
    <privapp-permissions package="com.orange.update">
        <permission name="android.permission.INSTALL_PACKAGES"></permission>
        <permission name="android.permission.WRITE_SECURE_SETTINGS"></permission>
    </privapp-permissions>

    <privapp-permissions package="com.orange.phone">
        <permission name="android.permission.CONTROL_INCALL_EXPERIENCE"></permission>
        <permission name="com.android.voicemail.permission.READ_VOICEMAIL"></permission>
        <permission name="com.android.voicemail.permission.WRITE_VOICEMAIL"></permission>
    </privapp-permissions>

    <privapp-permissions package="com.orange.aura.oobe">
        <permission name="android.permission.INSTALL_PACKAGES"></permission>
        <permission name="android.permission.PACKAGE_USAGE_STATS"></permission>
        <permission name="android.permission.UPDATE_APP_OPS_STATS"></permission>
        <permission name="android.permission.WRITE_SECURE_SETTINGS"></permission>
    </privapp-permissions>

    <privapp-permissions package="com.facebook.system">
        <permission name="android.permission.CHANGE_COMPONENT_ENABLED_STATE"></permission>
        <permission name="android.permission.DELETE_PACKAGES"></permission>
        <permission name="android.permission.INSTALL_PACKAGES"></permission>
        <permission name="android.permission.REAL_GET_TASKS"></permission>
    </privapp-permissions>
</permissions>
